@model List<CodingEventsDemo.Models.EventCategory>

<h1>All Event Categories</h1>

<p>
    <a asp-controller="EventCategory" asp-action="Create">Create Category</a>
</p>

<table>
    <tr>
    <th>Category Name</th>
    </tr>
    @foreach(EventCategory category in Model)
    {
        <tr>
            <td>@category.Name</td>
        </tr>
    }
</table>